        a=int(input("Enter A: "))
        b=int(input("Enter B: "))
        add
        print(sum)
except NameError:
           print("sum does not exist")        

#IOError

def readFile():
    try:
        file = open('test.txt', 'r')
except EOFError as e:
    print(e)
except IOError as e:
readFile()
